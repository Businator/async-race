{
  "name": "online-store",
  "version": "1.1.0",
  "description": "Online Store",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve",
    "build": "tsc && cross-env NODE_ENV=production webpack",
    "lint": "npm run lint:styles && npm run lint:scripts",
    "lint:fix": "npm run lint:scripts:fix && npm run lint:styles:fix",
    "lint:scripts": "eslint --ext .ts,.tsx src",
    "lint:scripts:fix": "npm run lint:scripts -- --fix",
    "lint:styles": "stylelint \"src/**/*.css\"",
    "lint:styles:fix": "npm run lint:styles -- --fix",
    "test": "cross-env NODE_ENV=test jest",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk node_modules/.bin/jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/A-Hanchar/online-store.git"
  },
  "authors": [
    {
      "name": "Artsiom Hanchar",
      "email": "artsiom.hanchar@gmail.com",
      "url": "https://github.com/A-Hanchar"
    },
    {
      "name": "Marat Deniullov",
      "email": "deniullov@gmail.com",
      "url": "https://github.com/Businator"
    }
  ],
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.20.6",
    "@testing-library/dom": "^8.19.1",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "autoprefixer": "^10.4.13",
    "babel": "^6.23.0",
    "babel-loader": "^9.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.2",
    "cssnano": "^5.1.14",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "mini-css-extract-plugin": "^2.7.2",
    "netlify-webpack-plugin": "^1.1.3",
    "postcss": "^8.4.20",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^7.8.3",
    "postcss-pxtorem": "^6.0.0",
    "postcss-simple-extend": "^1.0.0",
    "prettier": "^2.8.1",
    "style-loader": "^3.3.1",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^5.0.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@types/testing-library__jest-dom": "^5.14.5",
    "jest-environment-jsdom": "^29.3.1",
    "ts-jest": "^29.0.3"
  }
}
